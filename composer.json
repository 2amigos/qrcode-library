{
    "name": "2amigos/yii2-qrcode-component",
    "description": "QrCode Generator",
    "type": "yii2-extension",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "component",
        "qrcode"
    ],
    "homepage": "http://yiiwheels.com/extension/qrcode-helper",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/2amigos/yii2-qrcode-helper/issues",
        "source": "https://github.com/2amigos/yii2-qrcode-helper"
